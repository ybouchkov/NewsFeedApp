# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'NewsFeedApp' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for NewsFeedApp
  pod 'SwiftLint'                                     # A tool to enforce Swift style and conventions. https://realm.io
  pod 'R.swift'
  pod 'IQKeyboardManager'
  pod 'Alamofire'
  pod 'SWRevealViewController', :inhibit_warnings => true
  pod 'R.swift.Library'                              #  https://github.com/mac-cain13/R.swift.Library
  pod 'TextFieldEffects'


  target 'NewsFeedAppTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'NewsFeedAppUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
